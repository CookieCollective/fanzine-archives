        <PAGEOBJECT XPOS="587.721102362205" YPOS="13175.0078740157" OwnPage="40" ItemID="262254256" PTYPE="4" WIDTH="265" HEIGHT="106.089448818897" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L265 0 L265 106.089 L0 106.089 L0 0 Z" copath="M0 0 L265 0 L265 106.089 L0 106.089 L0 0 Z" gXpos="587.721102362205" gYpos="13175.0078740157" gWidth="0" gHeight="0" PSTYLE="Text" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Text" CPARENT="Text White"/>
                <ITEXT CPARENT="Text White" CH="These images are the results of a raymarching algorithm whose signed distance function ventures beyond Lipschitz continuity. The visual effects produced are like fragmented optical layers of a distorted lens."/>
                <para/>
                <para/>
                <ITEXT CPARENT="Author White" CH="Lon Denise"/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="13374.0576377953" OwnPage="41" ItemID="264822912" PTYPE="2" WIDTH="804" HEIGHT="567.826771653547" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228962407924817" LOCALSCY="0.228962407924817" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/Elie.png" IRENDER="0" EMBEDDED="0" path="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" copath="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" gXpos="118.142362204725" gYpos="13374.0576377954" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="149.071496062992" YPOS="13374.9552755906" OwnPage="41" ItemID="262266096" PTYPE="4" WIDTH="318.928503937008" HEIGHT="566.929133858268" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L318.929 0 L318.929 566.929 L0 566.929 L0 0 Z" copath="M0 0 L318.929 0 L318.929 566.929 L0 566.929 L0 0 Z" gXpos="149.071496062992" gYpos="9563.3036220472" gWidth="0" gHeight="0" PSTYLE="Code" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Code" LINESP="9" FONTSIZE="9" FEATURES="inherit shadowed" SCOLOR="Rich Black"/>
                <ITEXT FONTSIZE="10" CH="// Saturated Mounts"/>
                <para PARENT="Code" LINESP="9"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float mountsTime = 0.1;"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float stepTime = 2.7;"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float shapeTime = 216.84;"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float colorTime = 6.0;"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float sphere(vec3 p, float r) { return length(p) - r; }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float merge(float a, float b) { return min(a, b); }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float mounts(vec3 p) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="p.x += iTime * .1;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float h = sin(p.x) * sin(p.z * 10.) * 1.5 + 0.3"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="* sin(mountsTime * 2.3);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="return p.y - h; }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="float scene(vec3 p) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float s = sphere(p, 1.5);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="s = merge(s, mounts(p));"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="s = merge(s, sphere(p - vec3(0.,-1.,5.), 5.0));"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="return s; }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="mat2 rot2(float theta) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float c = cos(theta), s = sin(theta);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="return mat2(c, s, -s, c); }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="vec3 march(vec2 uv) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec3 p = vec3(0.,1.,-5.);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec3 d = vec3(uv, 1.);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="d.yz *= rot2(-0.2);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="d = normalize(d);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float st = mix(.01, .1, abs(uv.x + .5*uv.y + sin(stepTime)));"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float i = 0.;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="for (;i &lt; 1.;i+=st) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="float s = scene(p);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="p += s * d * 0.95;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="if (s&lt;.01) break;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="}"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec3 col = 0.5 + 0.5 * cos(colorTime + i * 3. + uv.xyx + vec3(0,2,4));"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="return (col-step(1., i)) * (1.-i); }"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="void mainImage( out vec4 fragColor, in vec2 fragCoord ) {"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec2 nuv = fragCoord/iResolution.xy;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec2 uv = (nuv - 0.5) * iResolution.xy"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="/ iResolution.y;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec3 e = vec3(1./iResolution.x,1./iResolution.y,0.);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="vec3 col = vec3(0.);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="int aasteps = 6;"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="for (int x = 0 ; x &lt; aasteps ; ++x)"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="for (int y = 0 ; y &lt; aasteps ; ++y)"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="col += march(uv + vec2(x, y)"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="* e.xy / float(aasteps));"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="col /= float(aasteps * aasteps);"/>
                <para PARENT="Code" LINESP="9"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="fragColor = vec4(col,1.0);"/>
                <para PARENT="Code" LINESP="9"/>
                <ITEXT FONTSIZE="10" CH="}"/>
                <para PARENT="Code" LINESP="9"/>
                <para LINESP="9"/>
                <para LINESP="11"/>
                <ITEXT CPARENT="Author White" CH="lie Michel"/>
                <para LINESP="15"/>
                <ITEXT CPARENT="Author White" CH="shadertoy.com/view/3sBSWW"/>
                <trail LINESP="15"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="14009.3329133858" OwnPage="43" ItemID="264842736" PTYPE="2" WIDTH="804" HEIGHT="567.826771653547" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228962407924817" LOCALSCY="0.228962407924817" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/ThomasHooper_M.png" IRENDER="0" EMBEDDED="0" path="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" copath="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" gXpos="118.142362204725" gYpos="14009.332913386" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="149.071496062992" YPOS="14036.0573228346" OwnPage="43" ItemID="262242416" PTYPE="4" WIDTH="339.527559055118" HEIGHT="256.942677165354" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L339.528 0 L339.528 256.943 L0 256.943 L0 0 Z" copath="M0 0 L339.528 0 L339.528 256.943 L0 256.943 L0 0 Z" gXpos="149.071496062992" gYpos="12765.5067716535" gWidth="0" gHeight="0" PSTYLE="Text" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Text" CPARENT="Text White Shadow"/>
                <ITEXT CPARENT="Text White" CH="A wonderful aspect of ray marching is the ease of trying unusual concepts; what happens if time only subjectively moves forward when we move forward?"/>
                <para/>
                <para/>
                <ITEXT CPARENT="Text White" CH="A ray marcher takes big jumps through a scene, and smaller and smaller steps as it approaches the geometry surface. If time accelerates by a fixed amount for each step, small or "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="large, strange things happen..."/>
                <para/>
                <para/>
                <ITEXT CPARENT="Text White" CH="As you near the surface you may find it gets "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="swept away, or you could jump right into "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="the middle of an object. This is highly "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="dependent on how the geometry moves "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="over time, static objects will behave "/>
                <para/>
                <ITEXT CPARENT="Text White" CH="normally, fast moving objects become"/>
                <para/>
                <ITEXT CPARENT="Text White" CH=" ever more distorted."/>
                <para/>
                <para/>
                <ITEXT CPARENT="Author White" CH="Thomas Hooper"/>
                <para/>
                <ITEXT CPARENT="Author White" CH="shadertoy.com/view/WtfGz7"/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="14644.6081889764" OwnPage="45" ItemID="264834240" PTYPE="2" WIDTH="804" HEIGHT="567.826771653547" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228962407924817" LOCALSCY="0.228962407924817" LOCALX="-0.131713675570081" LOCALY="2.18376459494688" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/ben-goat-2.png" IRENDER="0" EMBEDDED="0" LOCK="1" path="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" copath="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" gXpos="118.142362204725" gYpos="14644.6081889766" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>

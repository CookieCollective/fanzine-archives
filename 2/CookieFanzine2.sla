        <PAGEOBJECT XPOS="118.142362204725" YPOS="4481.0305511811" OwnPage="13" ItemID="59165472" PTYPE="2" WIDTH="802.771653543307" HEIGHT="567" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228629032258065" LOCALSCY="0.228629032258065" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/3508x2480_2019-03-25_22-27-47.png" IRENDER="0" EMBEDDED="0" path="M0 0 L802.772 0 L802.772 567 L0 567 L0 0 Z" copath="M0 0 L802.772 0 L802.772 567 L0 567 L0 0 Z" ImageRes="0" gXpos="118.142362204725" gYpos="4481.03055118109" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="196.803149606299" YPOS="4499.97248031496" OwnPage="13" ItemID="111557440" PTYPE="4" WIDTH="242" HEIGHT="521.25" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L242 0 L242 521.25 L0 521.25 L0 0 Z" copath="M0 0 L242 0 L242 521.25 L0 521.25 L0 0 Z" gXpos="196.803149606299" gYpos="3229.42192913384" gWidth="0" gHeight="0" PSTYLE="Text" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Text" FONT="Georgia Regular"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="This image is produced by a filter inspired by datamoshing. An activity that consists into modifying compressed datas."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="A digital video holds a complete list of colours every seconds. Between those keyframes there is relative images."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="Lists of pixels that are not colours, but ponderation vectors, describing how to update colour relative to previous images."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="When we modify the sequence of key frames and relative images, the video player improvise and produce an intriguing image. Beyond the aesthetic, what pleases is also that form reveals function. The way the compression algorithm is thought generate different graphic styles."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="Thus we can imagine an artistic way of compressing datas to produce willingly a particular visual effect."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="The background image is the result of an interpretation of data compression. Each pixel is updated if the distance between his colour and video's pixel colour is superior to a certain threshold."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" CH="The pixel is translated from his own colour. The gray value of a colour can transform into an angle, and then into a direction. The pixel drifts as it changes colour. As a painting, pixels flow along the screen."/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Author White" CH="Lon Denise"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Author White" FONTSIZE="10" CH="github.com/leon196/OpticalFlowExample"/>
                <para PARENT="Text"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="5116.37669291339" OwnPage="15" ItemID="59171136" PTYPE="2" WIDTH="802.771653543307" HEIGHT="567" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228629032258065" LOCALSCY="0.228629032258065" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/3508x2480_2019-03-25_23-26-44.png" IRENDER="0" EMBEDDED="0" path="M0 0 L802.772 0 L802.772 567 L0 567 L0 0 Z" copath="M0 0 L802.772 0 L802.772 567 L0 567 L0 0 Z" ImageRes="0" gXpos="118.142362204725" gYpos="5116.37669291338" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="607.291023622046" YPOS="5158.67590551181" OwnPage="16" ItemID="116052000" PTYPE="4" WIDTH="295.708976377954" HEIGHT="256.007874015748" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L295.709 0 L295.709 256.008 L0 256.008 L0 0 Z" copath="M0 0 L295.709 0 L295.709 256.008 L0 256.008 L0 0 Z" gXpos="607.291023622046" gYpos="5158.67590551181" gWidth="0" gHeight="0" PSTYLE="Code" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Code" FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="void main () {"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  vec2 uv = gl_FragCoord.xy/resolution.xy;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  vec4 video = texture2D(videoMap, uv);"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  vec2 unit = 1./resolution.xy;"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  float a = luminance(video.rgb) * TAU;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  uv += vec2(cos(a),sin(a)) * unit;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  a = luminance(texture2D(backbuffer, uv).rgb) * TAU;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  uv += vec2(cos(a),sin(a)) * unit;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  uv -= normalize(p) * unit;"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  vec4 frame = texture2D(backbuffer, uv);"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  float should = colorDistance(frame, video);"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  should = smoothstep(.3, .4, should);"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="  gl_FragColor = mix(frame, video, should - .01);"/>
                <para PARENT="Code"/>
                <ITEXT FONTSIZE="10" CH="}"/>
                <para PARENT="Code"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="5750.47874015748" OwnPage="17" ItemID="59173968" PTYPE="2" WIDTH="802.771653543307" HEIGHT="591" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.228840266118389" LOCALSCY="0.228840266118389" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/Composition 1_07727.png" IRENDER="0" EMBEDDED="0" path="M0 0 L802.772 0 L802.772 591 L0 591 L0 0 Z" copath="M0 0 L802.772 0 L802.772 591 L0 591 L0 0 Z" ImageRes="0" gXpos="118.142362204725" gYpos="5750.47874015747" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="156.158110236221" YPOS="5751.65196850394" OwnPage="17" ItemID="116054848" PTYPE="4" WIDTH="325.354330708661" HEIGHT="580.348031496063" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L325.354 0 L325.354 580.348 L0 580.348 L0 0 Z" copath="M0 0 L325.354 0 L325.354 580.348 L0 580.348 L0 0 Z" gXpos="156.158110236221" gYpos="3210.55086614173" gWidth="0" gHeight="0" PSTYLE="Text" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Text" LINESP="13" CPARENT="Text White Shadow" FEATURES="inherit" BGCOLOR="Rich Black"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="When I was a kid I remember putting a magnet onto the family TV screen to see what would happen, I was really curious since it was strictly forbidden by my parents. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="I was amazed by the flowing rainbows arcs attracted towards the magnetic field, it was like unveiling a whole new world behind the glass and scanlines, i was disturbing the primary use of our family cathodic device, and it was beautiful."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="After many years using digital screens I almost forgot about the magic of analogue CRT monitors. But its still there. With CRT monitors you can feel the light, feel the beams of electrons and colors, sense the electric fields pulsating. Its all organic and i can understand why TVs were so mesmerizing back then, and why I often fell asleep in front of the mighty warm beam."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="I dont use magnets anymore but circuit bent video processors, DIY video mixers, analogue cameras and video feedback to create organic scanlines scapes. I love the organic granularity of CRT screens, being able to directly modulate electric flows to create colorful organic compositions, guiding shapes with feedback, bouncing on analogue waves and welcoming accidents, digging layers and layers of signals to find the right vista."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Text"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="This picture is made by using a camera pointed at a CRT monitor which signal feed some hacked analogue video circuitry plugged into the same monitor and captured with a DSLR."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="A good old analogue feedback video loop, creating endless layers of colors and lights. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Text White" FONT="Consolas Regular" FONTSIZE="10" FEATURES="inherit" BGCOLOR="Rich Black" CH="I used this setup for live performances before, connecting the DSLR output directly to video projectors."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Author White" FEATURES="inherit" BGCOLOR="Rich Black" CH="Sbastien Labrunie"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Author White" FEATURES="inherit" BGCOLOR="Rich Black" CH="youtu.be/dDKjWbpoZ7c"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>

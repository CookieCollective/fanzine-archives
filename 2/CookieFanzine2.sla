        <PAGEOBJECT XPOS="621.869881889764" YPOS="12145.388976378" OwnPage="38" ItemID="262263136" PTYPE="4" WIDTH="264.733937007874" HEIGHT="229" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L264.734 0 L264.734 229 L0 229 L0 0 Z" copath="M0 0 L264.734 0 L264.734 229 L0 229 L0 0 Z" gXpos="621.869881889764" gYpos="12145.388976378" gWidth="0" gHeight="0" ALIGN="2" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="2"/>
                <ITEXT CPARENT="Text White" CH="With the apparition of 3D worlds editor, we had to invent new tools. Some of them are called gizmos, they are affordant geometric shapes that can translate, rotate and scale. When using them, we often drift the cursor and tear the mesh into a distorted dimension. It is a mistake, but it does spring an interesting aesthetic."/>
                <para PARENT="Text" ALIGN="2"/>
                <ITEXT CPARENT="Text White" CH="Here is multiple Winnie-the-Pooh getting crushed into a rigging vertex storm. The contradiction of a corpse getting pulverized and a familiar happy face adds a sophisticated feeling to these triangles"/>
                <ITEXT CPARENT="Text White" FONTSIZE="11" FEATURES="inherit shadowed" CH="."/>
                <para PARENT="Text" ALIGN="2"/>
                <para PARENT="Text" ALIGN="2"/>
                <ITEXT CPARENT="Author White" CH="Lon Denise"/>
                <para PARENT="Text" ALIGN="2"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.142362204725" YPOS="7656.58015748032" OwnPage="23" ItemID="264828576" PTYPE="2" WIDTH="804" HEIGHT="567.826771653545" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.230042918454936" LOCALSCY="0.230042918454936" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="images/robotbugs_clean.png" IRENDER="0" EMBEDDED="0" path="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" copath="M0 0 L804 0 L804 567.827 L0 567.827 L0 0 Z" gXpos="118.142362204725" gYpos="7656.58015748038" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>

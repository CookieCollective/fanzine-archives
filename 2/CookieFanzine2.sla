                <para PARENT="Text" ALIGN="0" LINESPMode="1"/>
                <para PARENT="Text" ALIGN="0" LINESPMode="1"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" FONTSIZE="10" CH="L'hagard voit dfiler"/>
                <para PARENT="Text" ALIGN="0" LINESPMode="1"/>
                <ITEXT CPARENT="Text White" FONT="Georgia Regular" FONTSIZE="10" CH="machines tissant leur toile"/>

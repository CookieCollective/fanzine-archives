            <StoryText>
                <DefaultStyle PARENT="Code" FEATURES="inherit shadowed" FCOLOR="White" SCOLOR="Rich Black" BGCOLOR="Rich Black"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="float rand (float x)"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="{return fract(sin(x)*256.43);}"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="float HexDist (vec2 uv) {"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv = abs(uv);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="   return max(dot(uv, normalize(hr)), uv.x);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="}"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="vec4 HexGrid (vec2 uv) {"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv *= detail;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec2 ga = mod(uv,hr)-hr*0.5;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec2 gb = mod(uv-hr*0.5, hr)-hr*0.5;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec2 guv = dot(ga,ga)&lt;dot(gb,gb) ? ga : gb;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  guv = fract(guv);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec2 id = uv-guv;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  guv.y = .5-HexDist(guv);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  return vec4(guv.x, guv.y, id.x, id.y);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="}"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="void mainImage( out vec4 fragColor, in vec2 fragCoord )"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="{"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec2 uv = 2.*(fragCoord.xy / iResolution.xy)-1.;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv.x *= iResolution.x / iResolution.y;"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv.x -= 0.25;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv = mix(uv, floor(uv*15.)/10., length(uv));"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  float hx = HexGrid(uv).x;"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  uv.y += fract(uv.x*5.);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec4 hc = HexGrid(uv);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  vec3 col = mix(vec3(rand(hc.z)*0.8, rand(hc.w), 1.),"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="                 vec3(1., rand(hc.z)*0.6, rand(hc.w)),"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="                 step(0.05,hc.y+sin(time)));"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  col *= smoothstep(0.05,0.4,hx+sin(time*2.));"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  col = pow(col,vec3(0.6));"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="  fragColor = vec4(col, 1.0);"/>
                <para PARENT="Code"/>
                <ITEXT FCOLOR="White" BGCOLOR="Rich Black" CH="}"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <para PARENT="Code"/>
                <ITEXT CPARENT="Author White" FCOLOR="White" BGCOLOR="Rich Black" CH="Flopine"/>
